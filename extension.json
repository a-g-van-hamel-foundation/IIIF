				"ext.iiif.tify.js"
			],
			"dependencies": [
				"ext.iiif.lib.tify",
				"ext.iiif.lib.annotorious"
			]
		},
		"ext.iiif.lib.tify": {
			"packageFiles": [
				"lib-tify/tify.js"
			],
			"styles": [
				"lib-tify/tify.css"
			]
		},
		"ext.iiif.lib.annotorious": {
