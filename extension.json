		"ChangeTagsListActive": [ "IIIFHooks::onRegisterTags" ],
		"ChangeTagsAllowedAdd":  [ "IIIFHooks::onChangeTagsAllowedAdd" ],		
		"CodeEditorGetPageLanguage": [ "IIIFHooks::onCodeEditorGetPageLanguage" ],
